      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import time\n",
        "\n",
        "class TrafficLight:\n",
        "    def __init__(self, green_time, yellow_time, red_time):\n",
        "        self.green_time = green_time\n",
        "        self.yellow_time = yellow_time\n",
        "        self.red_time = red_time\n",
        "        self.state = 'RED'\n",
        "        self.wait_times = []\n",
        "        self.queue_lengths = []\n",
        "\n",
        "    def change_light(self, vehicles):\n",
        "        if self.state == 'GREEN':\n",
        "            # Vehicles pass through the intersection\n",
        "            for _ in range(vehicles):\n",
        "                wait_time = np.random.randint(0, 5)  # Simulate wait time for each vehicle\n",
        "                self.wait_times.append(wait_time)\n",
        "            self.queue_lengths.append(0)  # No queue during green light\n",
        "            time.sleep(self.green_time)\n",
        "            self.state = 'YELLOW'\n",
        "        elif self.state == 'YELLOW':\n",
        "            self.queue_lengths.append(vehicles)  # All waiting vehicles during yellow\n",
        "            time.sleep(self.yellow_time)\n",
        "            self.state = 'RED'\n",
        "        else:\n",
        "            self.queue_lengths.append(vehicles)  # All waiting vehicles during red\n",
        "            time.sleep(self.red_time)\n",
        "            self.state = 'GREEN'\n",
        "\n",
        "def simulate_traffic(light, duration):\n",
        "    start_time = time.time()\n",
        "    while time.time() - start_time < duration:\n",
        "        # Simulate a random number of vehicles arriving\n",
        "        vehicles = np.random.randint(1, 10)  # Random number of vehicles arriving\n",
        "        light.change_light(vehicles)\n",
        "\n",
        "# Example usage\n",
        "traffic_light = TrafficLight(green_time=30, yellow_time=5, red_time=30)\n",
        "simulate_traffic(traffic_light, duration=120)  # Simulate for 2 minutes\n",
        "\n",
        "# Visualization\n",
        "plt.figure(figsize=(12, 6))\n",
        "\n",
        "# Plot wait times\n",
        "plt.subplot(2, 1, 1)\n",
        "plt.plot(traffic_light.wait_times, label='Wait Times', color='blue')\n",
        "plt.title('Vehicle Wait Times at Traffic Light')\n",
        "plt.xlabel('Vehicle Number')\n",
        "plt.ylabel('Wait Time (seconds)')\n",
        "plt.legend()\n",
        "\n",
        "# Plot queue lengths\n",
        "plt.subplot(2, 1, 2)\n",
        "plt.plot(traffic_light.queue_lengths, label='Queue Lengths', color='red')\n",
        "plt.title('Queue Lengths at Traffic Light')\n",
        "plt.xlabel('Traffic Light Cycle')\n",
        "plt.ylabel('Number of Vehicles')\n",
        "plt.legend()\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ]
    }
